import React, { Component } from 'react';

class Greeting extends Component {
  render() {
    return (
      <div>
        <h1>Hello {this.props.name}!</h1>
      </div>
    );
  }
}

class LotsOfGreetings extends Component {
  render() {
    return (
      <div >
        <Greeting name='Nguyen' />
        <Greeting name='Hoang' />
      </div>
    );
  }
}
export default LotsOfGreetings;
